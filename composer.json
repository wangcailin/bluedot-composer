    {
      "name": "bluedot",
      "email": "cailin.wang@blue-dot.cn"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^3.0",
    "laravel/framework": "^8.83",
    "laravel/tinker": "^2.7",
    "overtrue/laravel-wechat": "^6.0",
    "spatie/laravel-query-builder": "4.0.3",
    "aliyuncs/oss-sdk-php": "^2.4",
    "textalk/websocket": "^1.5",
    "workerman/gateway-worker": "^3.0",
    "phpoffice/phpspreadsheet": "^1.23",
    "maatwebsite/excel": "^3.1",
    "phpmailer/phpmailer": "^6.6",
    "intervention/image": "^2.7",
    "guzzlehttp/guzzle": "^7.4",
    "spatie/laravel-permission": "^5.5",
    "laravel/passport": "^10.4",
    "alibabacloud/darabonba-openapi": "^0.2.8",
    "alibabacloud/dysmsapi-20170525": "^2.0",
    "alibabacloud/sdk": "^1.8",
    "tymon/jwt-auth": "1.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Composer\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Composer\\ComposerServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "easywechat-composer/easywechat-composer": true
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.7"
  }
}
