{
  "name": "bluedot/composer",
  "authors": [
    {
      "name": "bluedot",
      "email": "cailin.wang@blue-dot.cn"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "laravel/tinker": "^2.8",
    "laravel/passport": "^11.8",
    "spatie/laravel-query-builder": "^5.2",
    "spatie/laravel-permission": "^5.10",
    "overtrue/laravel-wechat": "^7.0",
    "phpmailer/phpmailer": "^6.7",
    "guzzlehttp/guzzle": "^7.5",
    "aliyuncs/oss-sdk-php": "^2.6",
    "bluedot/lookstar-client": "^1.6",
    "tymon/jwt-auth": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Composer\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Composer\\ComposerServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "easywechat-composer/easywechat-composer": true
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.7"
  }
}
